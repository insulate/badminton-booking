{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "seed:clear": "node seeders/clearAll.seeder.js",
    "seed:users": "node seeders/users.seeder.js",
    "seed:settings": "node seeders/settings.seeder.js",
    "seed:courts": "node seeders/courts.seeder.js",
    "seed:timeslots": "node seeders/timeslots.seeder.js",
    "seed:players": "node seeders/players.seeder.js",
    "seed:categories": "node seeders/categories.seeder.js",
    "seed:products": "node seeders/products.seeder.js",
    "seed:all": "node seeders/clearAll.seeder.js && node seeders/users.seeder.js && node seeders/settings.seeder.js && node seeders/courts.seeder.js && node seeders/timeslots.seeder.js && node seeders/players.seeder.js && node seeders/categories.seeder.js && node seeders/products.seeder.js",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "~1.9.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "axios": "^1.13.2",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "testTimeout": 30000
  }
}
